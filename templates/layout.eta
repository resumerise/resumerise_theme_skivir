<!doctype html>
<html class="<%= it.type %>">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">
		<title><%= it.resume.basics?.name ? it.resume.basics.name : ''%></title>
		<style>
			<%~ it.css %>
			<%~ it.widgetCss %>
		</style>
	</head>
	<body>
		<div class="l-main bd-container">
			<% it.templates.forEach(function(template){ %>
				<%~ include(template, {
					resume: it.resume,
					formatDate: it.formatDate
				}) %>
			<% }) %>
		</div>
	</body>
</html>
