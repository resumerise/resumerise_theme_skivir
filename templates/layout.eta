<!doctype html>
<html class="<%= it.type %>">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">
		<title><%= it.resume.basics?.name ? it.resume.basics.name : ''%></title>
		<style>
			<%~ it.css %>
			<%~ it.widgetCss %>
		</style>
	</head>
	<body>
		<div class="l-main bd-container">
			<!-- BASICS -->
			<%~ include("basic", {
				resume: it.resume,
				formatDate: it.formatDate
			}) %>

			<div class="container__wrapper">
				<div class="left">
					<!-- WORK -->
					<%~ include("work", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- PROJECT -->
					<%~ include("project", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- EDUCATION -->
					<%~ include("education", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- PUBLICATION -->
					<%~ include("publication", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
				</div>

				<div class="right">
					<!-- SKILLS -->
					<%~ include("skills", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- AWARDS -->
					<%~ include("awards", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- PROFILE -->
					<%~ include("profile", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- INTEREST -->
					<%~ include("interest", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- LANGUAGE -->
					<%~ include("language", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- VOLUNTEER -->
					<%~ include("volunteer", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- CERTIFICATION -->
					<%~ include("certification", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
					
					<!-- REFERENCE -->
					<%~ include("reference", {
						resume: it.resume,
						formatDate: it.formatDate
					}) %>
				</div>
			</div>
		</div>
	</body>
</html>